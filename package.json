{
  "name": "@tsed/mongoose-example",
  "version": "1.0.0",
  "description": "Here an example project with Mongoose and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "^6.103.2",
    "@tsed/common": "^6.103.2",
    "@tsed/core": "^6.103.2",
    "@tsed/di": "^6.103.2",
    "@tsed/exceptions": "^6.103.2",
    "@tsed/mongoose": "^6.103.2",
    "@tsed/platform-express": "^6.103.2",
    "@tsed/swagger": "^6.103.2",
    "@tsed/testing-mongoose": "^6.103.2",
    "@types/swagger-schema-official": "^2.0.22",
    "body-parser": "^1.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "mongoose": "^6.2.4",
    "serve-static": "^1.14.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/http-proxy": "^1.17.8",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@types/request-promise": "^4.1.48",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^7.0.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  }
}
